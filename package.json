{
  "name": "magical-forest-architect",
  "version": "1.0.0",
  "description": "Procedural landscape generator",
  "main": "src/main.js",
  "scripts": {
    "start": "npm run serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "node_modules/.bin/budo src/main.js:public/bundle.js --live -v -p 8080",
    "serve:prod": "node ./src/prod-server.js",
    "build": "browserify src/main.js | uglifyjs -m -c warnings=false > ./public/bundle.js",
    "build:webpack": "webpack -p",
    "serve:webpack": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --colors --config ./webpack.config.js"
  },
  "keywords": [
    "webgl",
    "three",
    "procedural",
    "shaders"
  ],
  "author": "Jonathan Grangien",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^9.3.0",
    "@types/three": "^0.89.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-glslify": "^2.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "budo": "^9.4.5",
    "css-loader": "^0.26.1",
    "dat-gui": "^0.5.0",
    "express": "^4.15.3",
    "glsl-noise": "0.0.0",
    "glslify": "^6.0.1",
    "glslify-fancy-imports": "^1.0.1",
    "glslify-hex": "^2.1.1",
    "glslify-live": "^2.1.1",
    "glslify-loader": "^1.0.2",
    "node-sass": "^3.13.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "three": "^0.84.0",
    "three-trackballcontrols": "^0.0.7",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.6.2",
    "uglify-js": "^3.3.5",
    "webpack": "^3.10.0",
    "webpack-glsl-loader": "^1.0.1"
  },
  "devDependencies": {
    "webpack-dev-middleware": "^2.0.4",
    "webpack-dev-server": "^2.10.1",
    "webpack-hot-middleware": "^2.21.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "glslify"
    ]
  }
}
