{
  "name": "magical-forest-architect",
  "version": "1.0.0",
  "description": "Procedural landscape generator",
  "main": "src/main.js",
  "scripts": {
    "start": "npm run serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "node_modules/.bin/budo src/main.js:public/bundle.js --live -v -p 8080",
    "build": "browserify src/main.js | uglifyjs -m -c warnings=false > public/bundle.js",
    "build:webpack": "webpack -p",
    "serve:webpack": "node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --colors --config ./webpack.config.dev.js"
  },
  "keywords": [
    "webgl",
    "three",
    "procedural",
    "shaders"
  ],
  "author": "Jonathan Grangien",
  "license": "ISC",
  "dependencies": {
    "babel-loader": "^6.2.9",
    "babel-plugin-glslify": "^2.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.0.0",
    "budo": "^9.4.5",
    "css-loader": "^0.26.1",
    "dat-gui": "^0.5.0",
    "glsl-noise": "0.0.0",
    "glslify": "^6.0.1",
    "glslify-fancy-imports": "^1.0.1",
    "glslify-hex": "^2.1.1",
    "glslify-live": "^2.1.1",
    "node-sass": "^3.13.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "three": "^0.84.0",
    "three-trackballcontrols": "^0.0.5",
    "uglify-js": "^2.7.5",
    "webpack": "^1.14.0",
    "webpack-glsl-loader": "^1.0.1"
  },
  "devDependencies": {
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.2"
  },
  "browserify": {
    "transform": [
      "babelify",
      "glslify"
    ]
  }
}
